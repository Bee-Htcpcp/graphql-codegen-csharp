#region interfaces

{{#each interfaces}}
  {{~> interfaces }}
{{/each}}

#endregion interfaces

#region types

{{#each types}}
  {{#ifCond name "==" 'Query'}}
    {{~> query }}
  {{else}}
    {{#ifCond name "==" 'Mutation'}}
      {{~> mutation }}
    {{else}}
      {{#ifCond name "==" 'Subscription'}}
          {{~> subscription }}
      {{else}}
          {{~> classes }}
      {{/ifCond}}
    {{/ifCond}}
  {{/ifCond}}
{{/each}}

#endregion types

#region inputTypes

{{#each inputTypes}}
  {{~> input }}
{{/each}}

#endregion inputTypes

#region enums

{{#each enums}}
  {{~> enum }}
{{/each}}

#endregion enums